{
    "name": "@ynn/eslint-plugin",
    "version": "0.0.3",
    "description": "Ynn eslint rules and plugins",
    "main": "dist/index.js",
    "author": "LvChengbin <lvchengbin59@gmail.com>",
    "license": "MIT",
    "keywords": [
        "eslint",
        "eslintplugin",
        "eslint-plugin"
    ],
    "scripts": {
        "clean": "rm -rf dist",
        "make": "yarn clean && tsc --declaration",
        "make:watch": "tsc --watch",
        "test": "yarn test:rules",
        "test:rules": "echo \"running test/rules.ts ...\" && ts-node test/rules/index.ts",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "lint": "eslint src/**/*.ts test/**/*.ts --cache --fix",
        "prepare": "yarn lint && yarn test && yarn make"
    },
    "engines": {
        "node": ">=14.15.0"
    },
    "publicConfig": {
        "access": "public"
    },
    "devDependencies": {
        "@types/eslint": "^7.2.6",
        "@types/estree": "^0.0.45",
        "@types/jest": "^26.0.19",
        "@types/node": "^14.14.19",
        "@types/yargs": "^15.0.12",
        "@typescript-eslint/eslint-plugin": "^4.11.1",
        "@typescript-eslint/parser": "^4.11.1",
        "cli-style": "^0.1.1",
        "eslint": "^7.17.0",
        "jest": "^26.6.3",
        "ts-jest": "^26.4.4",
        "ts-node": "^9.1.1",
        "typescript": "^4.1.3",
        "yargs": "^16.2.0"
    },
    "files": [
        "dist"
    ],
    "peerDependencies": {
        "@typescript-eslint/eslint-plugin": "*",
        "@typescript-eslint/parser": "*",
        "eslint": "*"
    },
    "dependencies": {
        "find-up": "^5.0.0"
    }
}
